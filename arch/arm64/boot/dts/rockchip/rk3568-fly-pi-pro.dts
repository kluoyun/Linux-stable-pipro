&sdmmc0 {
	max-frequency = <150000000>;
	supports-sd;
	bus-width = <4>;
	cap-mmc-highspeed;
	cap-sd-highspeed;
	disable-wp;
	cd-gpios = <&gpio0 RK_PA4 GPIO_ACTIVE_LOW>;
	sd-uhs-sdr104;
	vmmc-supply = <&vcc3v3_sd>;
	vqmmc-supply = <&vccio_sd>;
	pinctrl-names = "default";
	pinctrl-0 = <&sdmmc0_bus4 &sdmmc0_clk &sdmmc0_cmd &sdmmc0_det>;
	status = "okay";
};


&spi1 {
	/* ST7789V3 */
	#address-cells = <1>;
	#size-cells = <0>;
	pinctrl-names = "default";
//	pinctrl-0 = <&gpio3 RK_PC4 GPIO_ACTIVE_LOW &spi1m1_pins>;
	pinctrl-0 = <&spi1m1_pins>;
	status = "okay";
	max-freq = <50000000>;

    display1@st7789v {
        compatible = "sitronix,st7789v";
        reg = <0>;
 
        spi-max-frequency = <20000000>;
        rotate = <270>;
		height = <172>;
        width = <320>;
        bgr;
        fps = <10>;
        buswidth = <8>;
		init = <0x01000011
				0x02000078
				0x0100003A 0x05
				0x010000B2 0x0C 0x0C 0x00 0x33 0x33
				0x010000B7 0x35
				0x010000C2 0x01 0xFF
				0x010000C3 0x17
				0x010000C4 0x20
				0x010000BB 0x17
				0x010000C5 0x20
				0x010000D0 0xA4 0xA1
				0x01000029>;
		gamma = "D0 00 14 15 13 2C 42 43 4E 09 16 14 18 21\nD0 00 14 15 13 0B 43 55 53 0C 17 14 23 20";
        dc-gpios = <&gpio3 RK_PB7 GPIO_ACTIVE_LOW>;
        reset-gpios = <&gpio3 RK_PC5 GPIO_ACTIVE_LOW>;
        led-gpios = <&gpio3 RK_PC4 GPIO_ACTIVE_HIGH>;
        // reset-gpios = <&pio 1 7 GPIO_ACTIVE_LOW>;
        // dc-gpios = <&pio 1 5 GPIO_ACTIVE_LOW>;
        debug = <7>; //等级0～7 越高信息越多
    };
};

&tsadc {
	rockchip,hw-tshut-mode = <1>;
	rockchip,hw-tshut-polarity = <0>;
	status = "okay";
};

&uart2 {
	status = "okay";
};

&usb_host0_ehci {
	status = "okay";
};

&usb_host0_ohci {
	status = "okay";
};

&usb_host0_xhci {
	extcon = <&usb2phy0>;
	status = "okay";
};

&usb_host1_ehci {
	status = "okay";
};

&usb_host1_ohci {
	status = "okay";
};

&usb_host1_xhci {
	status = "okay";
};

&usbhost30 {
	status = "okay";
};

&usb2phy0 {
	status = "okay";
};

&usb2phy0_host {
	phy-supply = <&vcc5v0_usb_host>;
	status = "okay";
};

&usb2phy0_otg {
	phy-supply = <&vcc5v0_usb_otg>;
	status = "okay";
};

&usb2phy1 {
	status = "okay";
};

&usb2phy1_host {
	phy-supply = <&vcc5v0_usb_host>;
	status = "okay";
};

&usb2phy1_otg {
	phy-supply = <&vcc5v0_usb_host>;
	status = "okay";
};
